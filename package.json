{
  "name": "website-health-checker",
  "version": "1.0.0",
  "description": "test and guess if website is down or bought by another person",
  "scripts": {
    "test": "jshint . && eslint . && mocha",
    "coverage": "istanbul cover _mocha",
    "coveralls": "npm run coverage -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "bin": "src/bin/cli.js",
  "main": "src/lib/index.js",
  "keywords": [
    "website",
    "monitoring",
    "guess",
    "health"
  ],
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^5.4.0",
    "istanbul": "^0.4.5",
    "jshint": "~2.9.5",
    "mocha": "^4.0.1"
  },
  "dependencies": {
    "check-url": "^1.0.0",
    "cli-color": "^1.3.0",
    "cli-table": "^0.3.1",
    "is-honcode-certified": "0.0.4",
    "request": "^2.88.0",
    "unfluff": "^3.2.0",
    "yargs": "^10.0.3"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "globals": {
      "it": true,
      "describe": true
    },
    "rules": {
      "no-console": "off"
    }
  },
  "eslintIgnore": [
    "node_modules/**",
    "coverage/**"
  ],
  "engines": {
    "node": ">=6"
  },
  "author": "Adrion <adrion.dev@gmail.com> (cedricfrossard@gmail.com)",
  "license": "MIT"
}
